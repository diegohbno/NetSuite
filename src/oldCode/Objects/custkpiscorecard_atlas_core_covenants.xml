<kpiscorecard scriptid="custkpiscorecard_atlas_core_covenants">
  <description>Scorecards are intelligence tools that allow users to monitor key information and performance metrics of senior management, teams and individuals in real-time. Scorecards consolidate information into useful displays that give users the big picture, enabling managers to make critical decisions in a timely fashion.</description>
  <name>Covenants</name>
  <useperiods>T</useperiods>
  <custom>
    <kpi1></kpi1>
    <kpi10></kpi10>
    <kpi2></kpi2>
    <kpi3></kpi3>
    <kpi4></kpi4>
    <kpi5></kpi5>
    <kpi6></kpi6>
    <kpi7></kpi7>
    <kpi8></kpi8>
    <kpi9></kpi9>
  </custom>
  <audience>
    <allemployees>F</allemployees>
    <allpartners>F</allpartners>
    <allroles>T</allroles>
  </audience>
  <kpis>
    <kpi>
      <formula>{PROFIT}/({BANKBAL}+{RECEIVABLES}+{OTHERCURRENTASSET}+{FIXEDASSET}+{OTHERASSET})</formula>
      <hidden>F</hidden>
      <kpi>FORMULAPERCENT</kpi>
      <label>Return on Assets</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>{PROFIT}/{EQUITY}</formula>
      <hidden>F</hidden>
      <kpi>FORMULAPERCENT</kpi>
      <label>Return on Equity</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>({EQUITY})/({BANKBAL}+{RECEIVABLES}+{OTHERCURRENTASSET}+{FIXEDASSET}+{OTHERASSET})</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Equity Ratio</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>({PAYABLES}+{CREDITCARDBAL}+{OTHERCURRENTLIAB}+{LONGTERMLIAB})/{EQUITY}</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Debt to Equity</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>({PAYABLES}+{CREDITCARDBAL}+{OTHERCURRENTLIAB}+{LONGTERMLIAB})/({BANKBAL}+{RECEIVABLES}+{OTHERCURRENTASSET}+{FIXEDASSET}+{OTHERASSET})</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Debt to Total Assets</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>{CUSTOM}+{PROFIT}</formula>
      <hidden>F</hidden>
      <kpi>FORMULACURRENCY</kpi>
      <label>Equity Transactions</label>
      <lessismore>F</lessismore>
    </kpi>
  </kpis>
  <ranges>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>THIS_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>LAST_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>THIS_FISCAL_QUARTER_TO_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>LAST_FISCAL_QUARTER_TO_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>THIS_FISCAL_YEAR_TO_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>LAST_FISCAL_YEAR_TO_PERIOD</range>
    </range>
  </ranges>
