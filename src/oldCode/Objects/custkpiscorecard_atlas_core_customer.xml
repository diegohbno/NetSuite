<kpiscorecard scriptid="custkpiscorecard_atlas_core_customer">
  <description>Customer Scorecard  introduces customer metrics into performance management systems. Scorecards feature all manner of wonderful objectives relating to the customer value proposition and customer outcome metrics.</description>
  <name>Customer</name>
  <useperiods>F</useperiods>
  <custom>
    <kpi1>[scriptid=customsearch_atlas_lost_cust_rpt]</kpi1>
    <kpi10></kpi10>
    <kpi2></kpi2>
    <kpi3>[scriptid=customsearch_atlas_customer_return_value]</kpi3>
    <kpi4></kpi4>
    <kpi5></kpi5>
    <kpi6></kpi6>
    <kpi7></kpi7>
    <kpi8></kpi8>
    <kpi9></kpi9>
  </custom>
  <audience>
    <allemployees>F</allemployees>
    <allpartners>F</allpartners>
    <allroles>T</allroles>
  </audience>
  <kpis>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>NEWCUSTOMERSORD</kpi>
      <label></label>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>CUSTOM</kpi>
      <label>Lost Customers</label>
    </kpi>
    <kpi>
      <formula>{NEWCUSTOMERSORD}-{CUSTOM}</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Net New Customers</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>BOOKINGS</kpi>
      <label></label>
    </kpi>
    <kpi>
      <formula>{SALES}/{NEWCUSTOMERSORD}</formula>
      <hidden>F</hidden>
      <kpi>FORMULACURRENCY</kpi>
      <label>Sales Value Per Customer</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>CUSTOM3</kpi>
      <label>Value of Customer Returns</label>
    </kpi>
    <kpi>
      <formula>{TOTALBOOKINGS}/{TOTALORDERS}</formula>
      <hidden>F</hidden>
      <kpi>FORMULACURRENCY</kpi>
      <label>Average Deal Size</label>
      <lessismore>F</lessismore>
    </kpi>
  </kpis>
  <ranges>
