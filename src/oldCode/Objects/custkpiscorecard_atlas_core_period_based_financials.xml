<kpiscorecard scriptid="custkpiscorecard_atlas_core_period_based_financials">
  <description>Scorecards are an intelligent tool allows users to monitor key information in real-time, from senior management down to the performance of teams and individuals. It consolidates information into useful information that gives users the big picture, enabling managers to make critical decisions at the right time.</description>
  <name>Period Based Financials</name>
  <useperiods>T</useperiods>
  <custom>
    <kpi1></kpi1>
    <kpi10></kpi10>
    <kpi2></kpi2>
    <kpi3></kpi3>
    <kpi4></kpi4>
    <kpi5></kpi5>
    <kpi6></kpi6>
    <kpi7></kpi7>
    <kpi8></kpi8>
    <kpi9></kpi9>
  </custom>
  <audience>
    <allemployees>F</allemployees>
    <allpartners>F</allpartners>
    <allroles>T</allroles>
  </audience>
  <kpis>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>SALES</kpi>
      <label></label>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>OPERATINGEXPENSES</kpi>
      <label></label>
    </kpi>
    <kpi>
      <formula>({BANKBAL}+{RECEIVABLES})/({PAYABLES}+{CREDITCARDBAL}+{OTHERCURRENTLIAB})</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Quick Ratio</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>({BANKBAL}+{RECEIVABLES}+{OTHERCURRENTASSET})/({PAYABLES}+{CREDITCARDBAL}+{OTHERCURRENTLIAB})</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Current Ratio</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>{SALES}/{RECEIVABLES}</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Receivables Turnover</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>{DAYS}/({SALES}/{RECEIVABLES})</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Days Sales Outstanding</label>
      <lessismore>T</lessismore>
    </kpi>
    <kpi>
      <formula>{PROFIT}/{SALES}</formula>
      <hidden>F</hidden>
      <kpi>FORMULAPERCENT</kpi>
      <label>Profit Margin on Sales</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>{PROFIT}/{EQUITY}</formula>
      <hidden>F</hidden>
      <kpi>FORMULAPERCENT</kpi>
      <label>Return on Equity</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>({PAYABLES}+{CREDITCARDBAL}+{OTHERCURRENTLIAB}+{LONGTERMLIAB})/({BANKBAL}+{RECEIVABLES}+{OTHERCURRENTASSET}+{FIXEDASSET}+{OTHERASSET})</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Debt to Total Assets</label>
      <lessismore>T</lessismore>
    </kpi>
    <kpi>
      <formula>({PAYABLES}+{CREDITCARDBAL}+{OTHERCURRENTLIAB}+{LONGTERMLIAB})/{EQUITY}</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Debt to Equity</label>
      <lessismore>T</lessismore>
    </kpi>
  </kpis>
  <ranges>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>THIS_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype>VARIANCE_PERCENT</comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>LAST_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>THIS_FISCAL_QUARTER_TO_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>LAST_FISCAL_QUARTER_TO_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>THIS_FISCAL_YEAR_TO_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>LAST_FISCAL_YEAR_TO_PERIOD</range>
    </range>
  </ranges>
