<kpiscorecard scriptid="custkpiscorecard_atlas_core_customer_activity">
  <description>Scorecards are an intelligent tool allows users to monitor key information in real-time, from senior management down to the performance of teams and individuals. It consolidates information into useful information that gives users the big picture, enabling managers to make critical decisions at the right time.</description>
  <name>Customer Activity</name>
  <useperiods>F</useperiods>
  <custom>
    <kpi1></kpi1>
    <kpi10></kpi10>
    <kpi2>[scriptid=customsearch_atlas_lost_cust_rpt]</kpi2>
    <kpi3></kpi3>
    <kpi4>[scriptid=customsearch_atlas_events_rpt]</kpi4>
    <kpi5>[scriptid=customsearch_atlas_phonecalls_rpt]</kpi5>
    <kpi6>[scriptid=customsearch_atlas_tasks_rpt]</kpi6>
    <kpi7></kpi7>
    <kpi8></kpi8>
    <kpi9></kpi9>
  </custom>
  <audience>
    <allemployees>F</allemployees>
    <allpartners>F</allpartners>
    <allroles>T</allroles>
  </audience>
  <kpis>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>BOOKINGS</kpi>
      <label></label>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>NEWCUSTOMERSORD</kpi>
      <label></label>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>CUSTOM2</kpi>
      <label>Lost Customers</label>
    </kpi>
    <kpi>
      <formula>{NEWCUSTOMERSORD}-{CUSTOM2}</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Net New Customers</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>{BOOKINGS}/{NEWCUSTOMERSORD}</formula>
      <hidden>F</hidden>
      <kpi>FORMULACURRENCY</kpi>
      <label>Sales Value Per Customer</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>CUSTOM4</kpi>
      <label>Events</label>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>CUSTOM5</kpi>
      <label>Phone Calls</label>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>CUSTOM6</kpi>
      <label>Tasks</label>
    </kpi>
    <kpi>
      <formula>{CUSTOM4}+{CUSTOM5}+{CUSTOM6}</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Total Activities</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>{BOOKINGS}/({CUSTOM4}+{CUSTOM5}+{CUSTOM6})</formula>
      <hidden>F</hidden>
      <kpi>FORMULACURRENCY</kpi>
      <label>Sales Per Activity</label>
      <lessismore>F</lessismore>
    </kpi>
  </kpis>
  <ranges>
