      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>LAST_FISCAL_QUARTER_TO_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>THIS_FISCAL_YEAR_TO_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>LAST_FISCAL_YEAR_TO_PERIOD</range>
    </range>
  </ranges>
