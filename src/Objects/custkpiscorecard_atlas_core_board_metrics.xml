<kpiscorecard scriptid="custkpiscorecard_atlas_core_board_metrics">
  <description>Scorecards are intelligence tools that allow users to monitor key information and performance metrics of senior management, teams and individuals in real-time. Scorecards consolidate information into useful displays that give users the big picture, enabling managers to make critical decisions in a timely fashion.</description>
  <name>Board Metrics</name>
  <useperiods>T</useperiods>
  <custom>
    <kpi1></kpi1>
    <kpi10></kpi10>
    <kpi2></kpi2>
    <kpi3></kpi3>
    <kpi4></kpi4>
    <kpi5></kpi5>
    <kpi6></kpi6>
    <kpi7></kpi7>
    <kpi8></kpi8>
    <kpi9></kpi9>
  </custom>
  <audience>
    <allemployees>F</allemployees>
    <allpartners>F</allpartners>
    <allroles>T</allroles>
  </audience>
  <kpis>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>REVENUE</kpi>
      <label></label>
    </kpi>
    <kpi>
      <formula>{REVENUE}-{COGS}</formula>
      <hidden>F</hidden>
      <kpi>FORMULACURRENCY</kpi>
      <label>Gross Profit</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>({REVENUE}-{COGS})/{REVENUE}</formula>
      <hidden>F</hidden>
      <kpi>FORMULAPERCENT</kpi>
      <label>Gross Profit %</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>PROFIT</kpi>
      <label>Net Income</label>
    </kpi>
    <kpi>
      <formula>{PROFIT}/{REVENUE}</formula>
      <hidden>F</hidden>
      <kpi>FORMULAPERCENT</kpi>
      <label>Net Income as a Percent of Revenue</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>OPERCASHFLOW</kpi>
      <label></label>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>BANKBAL</kpi>
      <label></label>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>INVENTORY</kpi>
      <label>Inventory Value</label>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>NEWCUSTOMERS</kpi>
      <label></label>
    </kpi>
    <kpi>
      <formula>{SALES}/{RECEIVABLES}</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Receivables Turnover</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>({DAYS})/({SALES}/{RECEIVABLES})</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Days Sales Outstanding</label>
      <lessismore>T</lessismore>
    </kpi>
    <kpi>
      <formula>{COGS}/{INVENTORY}</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Inventory Turnover</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>{DAYS}/({COGS}/{INVENTORY})</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Days Inventory on Hand</label>
      <lessismore>T</lessismore>
    </kpi>
    <kpi>
      <formula>({BANKBAL}+{RECEIVABLES}+{OTHERCURRENTASSET})/({PAYABLES}+{CREDITCARDBAL}+{OTHERCURRENTLIAB})</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Current Ratio</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>{PROFIT}/({BANKBAL}+{RECEIVABLES}+{OTHERCURRENTASSET}+{FIXEDASSET}+{OTHERASSET})</formula>
      <hidden>F</hidden>
      <kpi>FORMULAPERCENT</kpi>
      <label>Return on Assets</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>({PAYABLES}+{CREDITCARDBAL}+{OTHERCURRENTLIAB}+{LONGTERMLIAB})/({BANKBAL}+{RECEIVABLES}+{OTHERCURRENTASSET}+{FIXEDASSET}+{OTHERASSET})</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Debt to Total Assets</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>({PAYABLES}+{CREDITCARDBAL}+{OTHERCURRENTLIAB}+{LONGTERMLIAB})/{EQUITY}</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Debt to Equity</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>({EQUITY})/({BANKBAL}+{RECEIVABLES}+{OTHERCURRENTASSET}+{FIXEDASSET}+{OTHERASSET})</formula>
      <hidden>F</hidden>
      <kpi>FORMULANUMERIC</kpi>
      <label>Equity Ratio</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>{PROFIT}/{EQUITY}</formula>
      <hidden>F</hidden>
      <kpi>FORMULAPERCENT</kpi>
      <label>Return on Equity</label>
      <lessismore>F</lessismore>
    </kpi>
  </kpis>
  <ranges>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>THIS_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
      <label></label>
      <range>LAST_PERIOD</range>
    </range>
    <range>
      <comparevalueto></comparevalueto>
