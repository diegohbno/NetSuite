<kpiscorecard scriptid="custkpiscorecard_atlas_core_financials_overview">
  <description>Scorecards are intelligence tools that allow users to monitor key information and performance metrics of senior management, teams and individuals in real-time. Scorecards consolidate information into useful displays that give users the big picture, enabling managers to make critical decisions in a timely fashion.</description>
  <name>Financials Overview</name>
  <useperiods>T</useperiods>
  <custom>
    <kpi1></kpi1>
    <kpi10></kpi10>
    <kpi2>[scriptid=customsearch_atlas_tax_int_dep_exp_rpt_2]</kpi2>
    <kpi3></kpi3>
    <kpi4></kpi4>
    <kpi5></kpi5>
    <kpi6></kpi6>
    <kpi7></kpi7>
    <kpi8></kpi8>
    <kpi9></kpi9>
  </custom>
  <audience>
    <allemployees>F</allemployees>
    <allpartners>F</allpartners>
    <allroles>T</allroles>
  </audience>
  <kpis>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>BANKBAL</kpi>
      <label></label>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>REVENUE</kpi>
      <label></label>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>COGS</kpi>
      <label>Cost of Goods Sold</label>
    </kpi>
    <kpi>
      <formula>{REVENUE}-{COGS}</formula>
      <hidden>F</hidden>
      <kpi>FORMULACURRENCY</kpi>
      <label>Gross Profit</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>({REVENUE}-{COGS})/{REVENUE}</formula>
      <hidden>F</hidden>
      <kpi>FORMULAPERCENT</kpi>
      <label>Gross Profit %</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>OPERATINGEXPENSES</kpi>
      <label>Operating Expenses</label>
    </kpi>
    <kpi>
      <formula>{EXPENSES}-{CUSTOM2}</formula>
      <hidden>F</hidden>
      <kpi>FORMULACURRENCY</kpi>
      <label>Expenses (excluding Interest, Tax, Depreciation and Amortization)</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <formula>{PROFIT}+{CUSTOM2}</formula>
      <hidden>F</hidden>
      <kpi>FORMULACURRENCY</kpi>
      <label>EBITDA</label>
      <lessismore>F</lessismore>
    </kpi>
    <kpi>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <hidden>F</hidden>
      <invertcomparison>F</invertcomparison>
      <kpi>PROFIT</kpi>
      <label>Net Income</label>
    </kpi>
    <kpi>
      <formula>{PROFIT}/{REVENUE}</formula>
      <hidden>F</hidden>
      <kpi>FORMULAPERCENT</kpi>
      <label>Net Income as a % of Revenue</label>
      <lessismore>F</lessismore>
    </kpi>
  </kpis>
  <ranges>
    <range>
      <comparevalueto></comparevalueto>
      <comparewithprevious>F</comparewithprevious>
      <comparisontype></comparisontype>
      <invertcomparison>F</invertcomparison>
